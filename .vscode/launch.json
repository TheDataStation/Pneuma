    "version": "0.2.0",
    "configurations": [
        {
            "name": "Reg Setup",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "setup --db_path=../out/storage.db",
        },
        {
            "name": "Reg File Read",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_tables --db_path=../out/storage.db ../sample_data/csv/5cq6-qygt.csv david",
        },
        {
            "name": "Reg Folder Read",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_tables --db_path=../out/storage.db ../sample_data/csv/ david",
        },
        {
            "name": "Reg S3 Read",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_tables --db_path=../out/storage.db [S3 URI] david csv s3 [REGION] [ACCESS KEY] [SECRET KEY]",
        },
        {
            "name": "Reg Context Add",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_metadata --db_path=../out/storage.db ../sample_data/context/sample_context.txt context ../sample_data/csv/5cq6-qygt.csv",
        },
        {
            "name": "Reg Summary Add",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_metadata --db_path=../out/storage.db ../sample_data/summary/sample_summary.txt summary ../sample_data/csv/5cq6-qygt.csv",
        },
        {
            "name": "Reg Metadata CSV Add",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_metadata --db_path=../out/storage.db ../sample_data/metadata.csv",
        },
        {
            "name": "Sum Summarize",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/summarizer",
            "program": "summarizer.py",
            "console": "integratedTerminal",
            "args": "summarize --db_path=../out/storage.db ../sample_data/csv/5cq6-qygt.csv",
        },
        {
            "name": "Sum Summarize All",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/summarizer",
            "program": "summarizer.py",
            "console": "integratedTerminal",
            "args": "summarize --db_path=../out/storage.db",
        },
        {
            "name": "Sum Purge Tables",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/summarizer",
            "program": "summarizer.py",
            "console": "integratedTerminal",
            "args": "purge_tables --db_path=../out/storage.db",
        },
        {
            "name": "Idx Generate",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/index_generator",
            "program": "index_generator.py",
            "console": "integratedTerminal",
            "args": "generate_index --db_path=../out/storage.db --index_path=../out/indexes sample_index '../sample_data/csv/5cq6-qygt.csv','../sample_data/csv/5n77-2d6a.csv'",
        },
        {
            "name": "Idx Generate All",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/index_generator",
            "program": "index_generator.py",
            "console": "integratedTerminal",
            "args": "generate_index --db_path=../out/storage.db --index_path=../out/indexes complete_index",
        },
        {
            "name": "Qry Query",
            "type": "debugpy",
            "python": "${workspaceFolder}/pneuma/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/pneuma/query",
            "program": "query.py",
            "console": "integratedTerminal",
            "args": "query --db_path=../out/storage.db --index_path=../out/indexes complete_index \"Why was this dataset created?\" 3",
        },
    ]
}