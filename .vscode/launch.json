    "version": "0.2.0",
    "configurations": [
        {
            "name": "Reg Setup",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${fileDirname}",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "setup --db_path=storage.db",
        },
        {
            "name": "Reg CSV Read",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${fileDirname}",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "read_csv --db_path=storage.db ../../data_src/benchmarks/context/chicago-open-data/BX1_chicago.csv",
        },
        {
            "name": "Reg Context Add",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${fileDirname}",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_context --db_path=storage.db ../../data_src/benchmarks/context/chicago-open-data/BX1_chicago.csv ./testing_data/test_context.txt",
        }
    ]
}