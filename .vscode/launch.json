    "version": "0.2.0",
    "configurations": [
        {
            "name": "Reg Setup",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/modules/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "setup --db_path=../out/storage.db",
        },
        {
            "name": "Reg File Read",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/modules/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "read_table --db_path=../out/storage.db ../sample_data/5cq6-qygt.csv david",
        },
        {
            "name": "Reg S3 Read",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/modules/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "read_table --db_path=../out/storage.db [S3 URI] david csv s3 [REGION] [ACCESS KEY] [SECRET KEY]",
        },
        {
            "name": "Reg Context Add",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/modules/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_context --db_path=../out/storage.db ../sample_data/5cq6-qygt.csv ../sample_data/sample_context.txt",
        },
        {
            "name": "Reg Summary Add",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/modules/registration",
            "program": "registration.py",
            "console": "integratedTerminal",
            "args": "add_summary --db_path=../out/storage.db ../sample_data/5cq6-qygt.csv ../sample_data/sample_summary.txt",
        },
        {
            "name": "Sum Summarize",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/modules/summarizer",
            "program": "summarizer.py",
            "console": "integratedTerminal",
            "args": "summarize --db_path=../out/storage.db ../sample_data/5cq6-qygt.csv",
        },
        {
            "name": "Idx Generate",
            "type": "debugpy",
            "python": "${workspaceFolder}/modules/env/Scripts/python.exe",
            "request": "launch",
            "cwd": "${workspaceFolder}/modules/index_generator",
            "program": "index_generator.py",
            "console": "integratedTerminal",
            "args": "generate_index --db_path=../out/storage.db sample_index '../sample_data/5cq6-qygt.csv','../sample_data/5n77-2d6a.csv'",
        },
    ]
}